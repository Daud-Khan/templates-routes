<script type="text/javascript">
    $(document).ready(function () {

        $('.searchtweet').click(function (e) {
            $('td').remove();
            e.preventDefault();
            var country = $("#country").val();
            var tweet = $(".searchbartweet").val();
            if (country == "") {
                console.log("yayyy");
                $.ajax({
                    method: "POST",
                    url: "http://localhost:3000/tweet",
                    data: {
                        tweet: tweet,
                    }
                }).done(function (res) {
                    var trhtml = '';
                    res.forEach(function (friend) {
                        trhtml += '<Tr><td>' + friend.text + '</td><td>' + friend.user.name +
                            '</td></tr>'
                        console.log(friend.text);
                    });
                    $('.generatedtweets').append(trhtml);
                });
            } else if(tweet==""){
                console.log("yayyy");
                $.ajax({
                    method: "POST",
                    url: "http://localhost:3000/countryTweets",
                    data: {
                        country: country,
                    }
                }).done(function (res) {
                    console.log(res);
                    var trhtml = '';
                    res.forEach(function (friend) {
                        trhtml += '<Tr><td>' + friend.text + '</td><td>' + friend.user.name +
                            '</td></tr>'
                        console.log(friend.text);
                    });
                    $('.generatedtweets').append(trhtml);
                });
            }else {
                $.ajax({
                    method: "POST",
                    url: "http://localhost:3000/countryWise",
                    data: {
                        tweet: tweet,
                        country: country
                    }
                }).done(function (res) {


                    var trhtml = '';
                    console.log(res);
                    res.forEach(function (friend) {
                        trhtml += '<tr><td>' + friend.text + '</td><td>' + friend.user.name +
                            '</td></tr>'

                    });
                    $('.generatedtweets').append(trhtml);
                });
            }
        });
        $('.mapBtn').on("click", function () {
            $('td').remove();

            var country = $('#country').val();
            $('.btn-warning').text("Selected Country : " + country);
            $.ajax({
                    method: "POST",
                    url: "http://localhost:3000/countryTweets",
                    data: {
                        country: country,
                    }
                }).done(function (res) {
                    console.log(res);
                    var trhtml = '';
                    res.forEach(function (friend) {
                        console.log(friend);
                        trhtml += '<Tr><td>' + friend.text + '</td><td>' + friend.user.name +
                            '</td></tr>'
                        console.log(friend.text);
                    });
                    $('.generatedtweets').append(trhtml);
                });
        })
        
        demo.initDashboardPageCharts();

    });
</script>

